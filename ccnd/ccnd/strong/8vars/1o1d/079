  P1 -> X6 : ;
  D1 -> X6 : ;
  X3 -> X6 : ;
  X4 -> X6 : ;
}

Constraints {
  X1 X6 : (v1 v2) (v3 v2) : ;
  X1 X6 : (v1 v2) (v3 v2) : !p1;
